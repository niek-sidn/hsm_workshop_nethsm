enable_set_attribute_value: false
log_level: Info

slots:
  - label: NitroNetHSM
    description: Nitrokey NetHSM

    operator:
      username: "${NS}~operator"
      password: "${PARTPASS_O}-${NS}"
    administrator:
      username: "${NS}~admin"
      password: "${PARTPASS_A}-${NS}"

    instances:
      - url: "https://${NETHSM_PUB_IPV4}:32768/api/v1"
        danger_insecure_cert: true

    retries:
      count: 30
      delay_seconds: 1

