# Keep synchronized with hsm_env_vars.envsubst
# To be sourced in by .bashrc
# After envsubst
export NETHSM_PUB_IPV4="${NETHSM_PUB_IPV4}"
export CURLOPTS=' --silent --insecure '
export API="https://${NETHSM_PUB_IPV4}:32768/api/v1"
export ADMIN="${NS}~admin"
export OPERATOR="${NS}~operator"
export APASS="${PARTPASS_A}-${NS}"
export OPASS="${PARTPASS_O}-${NS}"
export SO_NETHSM="/usr/local/lib/nethsm/nethsm-pkcs11-vv1.6.0-x86_64-ubuntu.24.04.so"
export SO_SOFTHSM="/usr/lib/softhsm/libsofthsm2.so"
